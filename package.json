{
	"name": "soma-server",
	"version": "1.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"packageManager": "npm@10.9.2",
	"scripts": {
		"build": "dotenv -e ~/.important/soma.env -- turbo run build",
		"dev": "dotenv -e ~/.important/soma.env -- turbo run dev",
		"start": "turbo run start",
		"lint": "turbo run lint",
		"test": "turbo run test",
		"clean": "turbo run clean",
		"typecheck": "turbo run typecheck"
	},
	"author": "zhen",
	"devDependencies": {
		"@eslint/js": "^9.28.0",
		"eslint": "^9.28.0",
		"globals": "^16.2.0",
		"turbo": "^2.5.3",
		"typescript-eslint": "^8.33.0"
	}
}
